<p style="color: green"><%= notice %></p>

<h1>Menus</h1>
<p><%= link_to "Categories", categories_path %></p> 


<div id="menus">
  <% @menus.each do |menu| %>
    <%= render menu %>
    <%= render menu.categories %>
    <p>
      <%= link_to "Show this menu", menu %>
    </p>
  <% end %>
</div>

<%= link_to "New menu", new_menu_path %>
